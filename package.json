{
  "name": "monadplus",
  "version": "1.0.0",
  "description": "Partial port of Haskell's MonadPlus",
  "main": "src/index.js",
  "scripts": {
    "coverage": "istanbul cover test/*.js",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "jshint .",
    "test": "tape test/*.js",
    "validate": "npm run lint && npm test && npm run coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jimf/monadplus"
  },
  "keywords": [
    "monad",
    "maybe"
  ],
  "author": "Jim Fitzpatrick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimf/monadplus/issues"
  },
  "homepage": "https://github.com/jimf/monadplus",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "data.maybe": "^1.2.2",
    "istanbul": "^0.4.3",
    "jshint": "^2.7.0",
    "ramda-fantasy": "^0.5.0",
    "sanctuary": "^0.11.1",
    "tape": "^4.0.0"
  }
}
